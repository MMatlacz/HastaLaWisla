             onclick="if(this.src === 'http://edi.iem.pw.edu.pl/~matlaczm/wawcode/Frontend/img/logo/HastaLaWislaNiskie.png') { this.src='img/logo/HastaLaWislaLunetaNiski.png'} else { this.src='img/logo/HastaLaWislaNiskie.png' }"
